              <DbSource ConnectionRef="GestionEmpresaConnectionString (MySettings)" DbObjectType="Unknown" GenerateShortCommands="true" GeneratorSourceName="SiguientePresupuesto" Modifier="Public" Name="SiguientePresupuesto" QueryType="Scalar" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataBy18" UserSourceName="SiguientePresupuesto">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>-- Parámetros esperados:
--   @Empresa NVARCHAR(50)
--   @fecha   INT   -- año (ej. 2025)

-- Si existen presupuestos válidos para esa empresa y año, calcula el siguiente correlativo.
-- Si no existen, arranca en EMP-0001-AA.
SELECT
    @Empresa + '-' +
    RIGHT('0000' + CAST(
        ISNULL(
            (SELECT MAX(CAST(SUBSTRING(p.Id_Presupuesto, LEN(@Empresa) + 2, 4) AS INT))
             FROM dbo.Presupuesto AS p
             WHERE p.Id_Empresa = @Empresa
               AND YEAR(p.Fecha) = @fecha
               AND ISNUMERIC(SUBSTRING(p.Id_Presupuesto, LEN(@Empresa) + 2, 4)) = 1),
            0
        ) + 1 AS VARCHAR(10)
    ), 4)
    + '-' + RIGHT(CAST(@fecha AS VARCHAR(4)), 2)             AS Siguiente,
